<?xml version="1.0" ?>
<GestIKParams version="1.0">
<!--ik solver params -->
	<parameter>
		<name>IK_alpha</name>
		<val>0.01</val>   
	</parameter>
	<parameter>
		<name>IK_maxSqError</name>
		<val>0.0001</val>   
	</parameter>
	<parameter>
		<name>IK_reachPct</name>
		<val>0.75</val>   
	</parameter>
	<parameter>
		<name>IK_solveIters</name>
		<val>100</val>   
	</parameter>
	<parameter>
		<name>IK_drawRad</name>
		<val>0.2</val>   
	</parameter>
	<parameter>
		<name>IK_elbowScale</name>
		<val>0.36</val>   
	</parameter>
	<parameter>
		<name>IK_fastTrajMult</name>
		<val>1.4</val>   
	</parameter>
	<parameter>
		<name>IK_ctrYOffset</name>
		<val>-0.05</val>   
	</parameter>
<!--trajectory gen params -->
	<parameter>
		<name>trajLenThresh</name>
		<val>0.01</val>   
	</parameter>
	<parameter>
		<name>trajNumReps</name>
		<val>5</val>   
	</parameter>
	<parameter>
		<name>trajDistMult</name>
		<val>.3</val>   
	</parameter>
	<parameter>
		<name>trajDesiredVel</name>
		<val>.03126</val>   
	</parameter>
	<parameter>
		<name>trajRandCtrStd</name>
		<val>.0157</val>   
	</parameter>
	<parameter>
		<name>trajRandCtrStdScale_X</name>
		<val>.1</val>   
	</parameter>
	<parameter>
		<name>trajRandCtrStdScale_Y</name>
		<val>1.0</val>   
	</parameter>
	<parameter>
		<name>trajRandCtrStdScale_Z</name>
		<val>1.5</val>   
	</parameter>
	<parameter>
		<name>trajRandSclStd</name>
		<val>.0001</val>   
	</parameter>
	<parameter>
		<name>trajNev4OvPct</name>
		<val>.2</val>   
	</parameter>
<!--window, cam and env params -->
	<parameter>
		<name>win_Width</name>
		<val>200</val>   
	</parameter>
	<parameter>
		<name>win_Height</name>
		<val>200</val>   
	</parameter>
	<parameter>
		<name>origZoom</name>
		<val>0.80</val> 
	</parameter>
	<parameter>
		<name>numTotSymPerLtr</name>
		<val>10</val> 
	</parameter>
	<parameter>
		<name>ltrIdxStSave</name>
		<val>0</val> 
	</parameter>
<!-- quality of motion blur : int from 0 to 5 (0 : off, val builds blur over neighboring 2*val + 1 frames -->
	<parameter>
		<name>motionBlurQual</name>
		<val>2</val>
	</parameter>
<!--random camera and skeleton values -->
<!--+/- camera orientation uniform range in radians-->
	<parameter>
		<name>rnd_camThet</name>
		<val>.15</val>
	</parameter>
<!-- +/- camera zoom uniform range -->
	<parameter>
		<name>rnd_camZoom</name>
		<val>0.01</val>
	</parameter>
<!-- camera uniform pct of translation dim - should be around 50-100 -->
	<parameter>
		<name>rnd_camTrans</name>
		<val>50</val>
	</parameter>
<!-- head color bound - grayscale : avoid rand colors of bkgrnd clr +/- this amt (.05-.25)-->
	<parameter>
		<name>rnd_headClrBnd</name>
		<val>.2</val>
	</parameter>
	<!-- +/- head dim mod pct -->
	<parameter>
		<name>rnd_headDimPct</name>
		<val>0.3</val>
	</parameter>
<!-- hand color bound - grayscale : avoid rand colors of bkgrnd clr +/- this amt (.05-.25)-->
	<parameter>
		<name>rnd_handClrBnd</name>
		<val>.2</val>
	</parameter>
<!-- +/- hand dim mod pct -->
	<parameter>
		<name>rnd_handDimPct</name>
		<val>0.4</val>
	</parameter>
<!--END random camera and skeleton values -->
<!--Set dataType 0 == const vel for all ltrs, 1 == fixed 16, 2 == mult8 train/test -->
	<parameter>
		<name>dataType</name>
		<val>1</val> 
	</parameter>
	<parameter>
		<name>bkgR</name>
		<val>.3</val> 
	</parameter>
	<parameter>
		<name>bkgG</name>
		<val>.3</val> 
	</parameter>
	<parameter>
		<name>bkgB</name>
		<val>.3</val> 
	</parameter>
	<parameter>
		<name>bkgA</name>
		<val>1.0</val> 
	</parameter>
<!-- output directory to write results to - defaults to dart\apps\gestureIK\frames if unspecified -->
	<parameter>
		<name>baseOutDir</name>
		<val>E:/Dropbox/Public/GestureIK/frames/</val>
	</parameter>
	<parameter>
		<name>fixedClipLen</name>
		<val>16</val>
	</parameter>
	<parameter>
		<name>IDX_useLeftHand</name>
		<val>FALSE</val> 
	</parameter>
<!-- whether or not to use xml-specified output directory -->
	<parameter>
		<name>IDX_useOutputDir</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_useMotionBlur</name>
		<val>FALSE</val> 
	</parameter>
	<parameter>
		<name>IDX_chgTrajDbgClrs</name>
		<val>FALSE</val> 
	</parameter>
<!-- randomization flags - which aspects of skeleton and environment to randomize -->
	<parameter>
		<name>IDX_rndCamOrient</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_rndCamLoc</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_rndHeadDims</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_rndHeadClr</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_rndHandShape</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_rndHandDims</name>
		<val>TRUE</val> 
	</parameter>
	<parameter>
		<name>IDX_rndHandClr</name>
		<val>TRUE</val> 
	</parameter>
</GestIKParams>
